@model IList<SECU_CAMSPlus_DEV.Models.Admin>

@using (Html.BeginForm())
{
    if (Model.Count > 0)
    {
    <table>
        <tbody>
            @for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>
                        @Html.CheckBoxFor(m => Model[i].IsChecked)
                        @Model[i].Text
                        @Html.HiddenFor(m => Model[i].Value)
                        @Html.HiddenFor(m => Model[i].Text)
                    </td>
                </tr>
            }
        </tbody>
    </table>}
    else
    {
    <div class="text-center mbot30">
        Escalation not added for this project
    </div>
    }
    
      <input class="btn btn-success btn-sm" type="submit"   name="Command" value="Save" id="btnfinish" onclick="return confirm('Are you sure you want to assign circle to user ?')" />

}
