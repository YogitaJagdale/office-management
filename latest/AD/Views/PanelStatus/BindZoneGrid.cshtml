@using (Html.BeginForm())
{
    @Html.Hidden("hdnaccessbit", Session["BinaryAccessBit"], new { @id = "hdnaccessbit" })
    @Html.Hidden("hdnaccessbitPanelControl", Session["BinaryAccessBitPanelControl"], new { @id = "hdnaccessbitPanelControl" })

    @Html.ValidationSummary(true)


    
    <div class="col-lg-12 animated fadeIn margin-top">
        <div class="panel panel-yellow">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-12 col-sm-7 col-md-7">
                        <h2>
                            <label>Client Name : &nbsp;</label>
                            @Html.LabelFor(m => m.Client_Name, Model.Client_Name)  &nbsp;  &nbsp;  &nbsp;<label>Panel Id :  &nbsp;</label>
                            @Html.LabelFor(m => m.PanelID, Model.PanelID)</h2>
                    </div>
                    <div class="clearfix"></div>
                </div>
            </div>
            <div class="col-md-12">
                <ul class="list-inline PanelStatus">
                    <li><span class="green"></span>Normal</li>
                    <li><span class="orange"></span>Alarmed</li>
                    <li><span class="darkYellow"></span>Faulted</li>
                    <li><span class="yellow"></span>Fault-Alarmed	</li>
                    <li><span class="blue"></span>Bypassed</li>
                    <li><span class="lightGreen"></span>Uninstalled</li>
                </ul>
            </div>
            <div class="clearfix"></div>
            <div class="panel-body">
                <div class="table-responsive" id="zonegrid">
                    <table id="ZoneDetailsDataTable" class="table table-hover">
                        <thead>
                            <tr>
                                <th>Zone No
                                </th>
                                <th>Sensor Type
                                </th>
                                <th>Location
                                </th>
                                <th>Zone Details
                                </th>
