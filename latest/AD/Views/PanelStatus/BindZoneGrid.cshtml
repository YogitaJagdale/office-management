<script>

    $(document).ready(function () {

        $("#btnBack").on("click", function (event) {
            event.preventDefault();
            history.back(1);
        });

        var SensorNo = "";
        var access = $('#hdnaccessbit').val();
        var accessPanelcontrol = $('#hdnaccessbitPanelControl').val();

        var accessbitArm = access.substr(2, 1);
        var accessbitFire = access.substr(3, 1);

        var accessbitReset = accessPanelcontrol.substr(0, 1);
        var accessbitSiren = accessPanelcontrol.substr(1, 1);
        var accessbitBypass = accessPanelcontrol.substr(2, 1);
        var accessbitCheckStatus = accessPanelcontrol.substr(3, 1);

        if (accessbitArm == 0) {

            $('#btnArmPanel').prop('disabled', true);
            $('#btnArmPanel').attr("title", "No Rights");
        }
        if (accessbitFire == 0) {

            $('#btnFireReset').prop('disabled', true);
            $('#btnFireReset').attr("title", "No Rights");
        }

        if (accessbitReset == 0) {

            $('#btnResetPanel').prop('disabled', true);
            $('#btnResetPanel').attr("title", "No Rights");
        }
        if (accessbitSiren == 0) {

            $('#btnSiren').prop('disabled', true);
            $('#btnSiren').attr("title", "No Rights");
        }
        if (accessbitBypass == 0) {

            $('#btnBypassSensor').prop('disabled', true);
            $('#btnBypassSensor').attr("title", "No Rights");
        }
        if (accessbitCheckStatus == 0) {

            $('#btnCheckStatus').prop('disabled', true);
            $('#btnCheckStatus').attr("title", "No Rights");
        }

        $("#btnDualAuthOk").on("click", function () {
            debugger;
            $('#divDualAuthPopUp').hide();
            $('#divDualAuthOverlay').hide();
            var cmd = "";

            if ($('#btnBypassName').text().trim() == 'Bypass') {
                cmd = 'byp';
                cmd = cmd + '|' + SensorNo;

            }
            else {
                cmd = 'uby';//unbypass                                                             
                cmd = cmd + "|" + SensorNo;

            }

            var url = '@Url.Action("DualAuth")';
            jQuery.ajax({
                url: url,
                type: 'POST',
                data: {

                    Login: $('#LoginID').val(),
                    password: $('#Password').val(),
                    Comments: $('#Comments').val()

                },
                success: function (data) {

                    if (data.Success) {

                        if (data.ResultRes == "Successs") {
                            jQuery.ajax({
                                url: '@Url.Action("BypassUnBypass")',
                                type: 'POST',
                                data: {

                                    loginid: $('#LoginID').val(),
                                    password: $('#Password').val(),
                                    cmd: cmd,
                                    ClientId: $("#hdnClientId").val(),
                                    PanelId: $("#hdnPanelId").val()

                                },
                                success: function (data) { $('#lblPControlErrorBypassRequest').html(data.Text); }
                            });
                        }
                        else {
                            $('#AuthResult').show();
                            $('#AuthResult').html(data.ResultRes);
                            $('#divDualAuthPopUp').show();
                            $('#divDualAuthOverlay').show();
                        }
                    }
                }
            });
        });


        $("#btnCancel").on("click", function () {
            $('#divDualAuthPopUp').hide();
            $('#divDualAuthOverlay').hide();
            $('#AuthResult').hide();
        });


        $("#btnBypassSensor").on("click", function () {
            debugger;
           
            if ($('#btnBypassName').text().trim() == 'Bypass') {
                
                $('#divDualAuthPopUp').show();
                $('#divDualAuthOverlay').show();//authentication needed
                $('#AuthResult').hide();
                $('#Comments').val = "";
            }
            else {

                $('#divUnbypassOverlay').show();
                $('#divUnbypassPopUp').show();//No authentication needed --only conformation for unbypass
                $('#AuthResult').hide();
                //$('#txtComments').val = "";
                $('#Comments').val = "";
             
            }

        });

        $("#btnUnbypassOk").on("click", function () { //Confirmation for unbypass

            $('#divUnbypassOverlay').hide();
            $('#divUnbypassPopUp').hide();
            var cmd = "";
            cmd = 'uby';//unbypass                                                             
            cmd = cmd + "|" + SensorNo;

            jQuery.ajax({
                url: '@Url.Action("BypassUnBypass")',
                    type: 'POST',
                    data: {

                        loginid: $('#LoginID').val(),
                        password: $('#Password').val(),
                        cmd: cmd,
                        ClientId: $("#hdnClientId").val(),
                        PanelId: $("#hdnPanelId").val()

                    },
                    success: function (data) { $('#lblPControlErrorBypassRequest').html(data.Text); }
            });

        });

        $("#btnUnbypassCancel").on("click", function () {
            $('#divUnbypassOverlay').hide();
            $('#divUnbypassPopUp').hide();
        });


        $("#ZoneDetailsDataTable tr ").click(function () {

            $("#ZoneDetailsDataTable tbody tr").removeClass('row_selected');

            $(this).addClass('row_selected');

            var SensorType = $(this).children().eq(1).text().trim();
            var SensorStatus = $(this).children().eq(5).text().trim();
            SensorNo = $(this).children().eq(0).text().trim();


            if (SensorType.indexOf('Fire') > -1) {
                if (accessbitFire != 0) {
                    $("#btnFireReset").show();
                }
                else {
                    $("#btnFireReset").show();
                    $('#btnFireReset').prop('disabled', true);
                    $('#btnFireReset').attr("title", "No Rights");
                }
            }

            else { $("#btnFireReset").hide(); }


            if (accessbitBypass != 0) {


                if (SensorStatus.indexOf('Bypassed') > -1) {

                    $("#btnBypassSensor").show();
                    $('#btnBypassName').text('Unbypass');
                    $('#btnBypassSensor').prop('title', 'Unbypass Sensor');
                    $("#SensorNo").text(SensorNo);
                    $("#lblSensorNo").show();
                    $("#SensorNo").show();

                }
                else {
                    $("#btnBypassSensor").show();
                    $('#btnBypassName').text('Bypass');
                    $('#btnBypassSensor').prop('title', 'bypass Sensor');
                    $("#lblSensorNo").show();
                    $("#SensorNo").text(SensorNo);
                    $("#SensorNo").show();

                }

                if (SensorStatus.indexOf('Uninstalled') > -1) {

                    $("#btnBypassSensor").hide();
                    $("#lblSensorNo").hide();
                    $("#SensorNo").hide();
                }
            }
            else {
                $("#btnBypassSensor").show();
                $('#btnBypassSensor').prop('disabled', true);
                $('#btnBypassSensor').attr("title", "No Rights");
            }

        });

        var armStatus = $('#lblPnlArmsStatus').val();

        //if (armStatus == "Armed") {
        //    $('#btnArmName').text('DissArm');
        //    $('#btnArmPanel').prop('title', 'DissArm Panel');

        //}else
         if (armStatus == "Disconnected") {
            $('#btnArmName').text('Arm Panel');
            $('#lblPnlArmsStatus').val('Disarmed');
         }

         if (armStatus == "Armed") {
             //$('#btnArmName').attr("style", "display:none");
