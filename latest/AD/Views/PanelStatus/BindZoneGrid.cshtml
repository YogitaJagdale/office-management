        $('#ZoneDetailsDataTable').DataTable({
            "lengthMenu": [[10, 20, 50, -1], [10, 20, 50, "All"]],
            "pageLength": 10,
            "fnRowCallback": function (nRow, aData, iDisplayIndex, iDisplayIndexFull) {
           
                count = $('#ZoneDetailsDataTable tr').size();
                switch (aData[5]) {
                    case 'Normal':
                        nRow.childNodes[1].bgColor = '#C5FF8A';//Fire Alarm---Red
                        nRow.childNodes[1].Color = 'White';
                        break;
                    case 'Alarmed':
                        nRow.childNodes[1].bgColor = '#FFBB55';//Intrusion Alarm---Orange
                        nRow.childNodes[1].Color = 'White';
                        break;
                    case 'Faulted':
                        nRow.childNodes[1].bgColor = 'Yellow';//System Fault--khaki
                        break;
                    case 'Fault-Alarmed':
                        nRow.childNodes[1].bgColor = '#FFFFCC';//Zone Fault--BlanchedAlmond
                        break;
                    case 'Bypassed':
                        nRow.childNodes[1].bgColor = '#99CCFF';//System Events
                        break;
                    case 'Uninstalled':
                        nRow.childNodes[1].bgColor = '#DBECDC';//Arming Disarming
