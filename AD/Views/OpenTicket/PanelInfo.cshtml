        });
    });
    $("#BtnClearText").on("click", function () {
        debugger;
        $("#txtNotepad").val('');
    });

    $("#btnEditCloseComents").on("click", function () {
        debugger;
        var comment = $('#txtAreaTicketHistoryClosureComment').val();
        $("#btnEditCloseComents").hide();
        $("#btnSaveClosureComment").show();
        $("#txtAreaTicketHistoryClosureComment").val('');
        $("#txtAreaTicketHistoryClosureComment").show();
        $.ajax({
            type: "POST",
            traditional: true,
            url: "TicketHistoryCloseComents",
            data: { sUserComment: comment },
            success: function (data) {
                if (data.Success) {
                    $('#TicketHistory').html(data.TicketHistory);
                    $('#PanelHistory').html(data.PanelHistory);
                }
            },
            error: function (reponse) {
                alert("error : " + reponse);
            }
        });
    });


    $("#btnSaveClosureComment").on("click", function () {
        debugger;
        var comment = $('#txtAreaTicketHistoryClosureComment').val();
        $("#btnSaveClosureComment").hide();
        $("#txtAreaTicketHistoryClosureComment").hide();
        $("#btnEditCloseComents").show();
        $.ajax({
            type: "POST",
            traditional: true,
            url: "SaveClosureComment",
            data: { sUserComment: comment },
            success: function (data) {
                if (data.Success) {
                    $('#TicketHistory').html(data.TicketHistory);
                    $('#PanelHistory').html(data.PanelHistory);
                }
            },
            error: function (reponse) {
                alert("error : " + reponse);
            }
        });
    });

    $("#btnTHistoryOldRecords").on("click", function () {
        debugger;
        $("#btnTHistoryOldRecords").attr("disabled", "disabled");
