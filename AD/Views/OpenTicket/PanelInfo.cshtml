    $(document).ready(function () {
        debugger;
        var SensorStatus = $('#hdnSensorStatus').val();

        if (SensorStatus == "Bypassed") {
            $("#btnBypassSensor").show();
            $('#btnBypassName').text('Unbypass');
            $('#btnBypassSensor').prop('title', 'Unbypass Sensor');
        }
        else {
            $("#btnBypassSensor").show();
            $('#btnBypassName').text('Bypass');
            $('#btnBypassSensor').prop('title', 'bypass Sensor');
        }

        if (SensorStatus == "Uninstalled") {
            $("#btnBypassSensor").hide();
        }

        var armStatus = $('#lblPnlArmsStatus').val();
        if (armStatus == "Armed") {
            $('#btnArmPanel').prop('disabled', true);
            $('#btnArmPanel').prop('title', 'Disarm panel not allowed');
        }
        else if (armStatus == "Disconnected") {
            $('#btnArmName').val('Arm Panel');
        }
        else { $('#btnArmName').val('Arm Panel'); }


        debugger;
        $('#btnEditCloseComents').hide();
        var elementId = "@ViewData["UserType"]";

        if (elementId == 1) {
            $("#btnEditCloseComents").show();
        }
        else {
            $("#btnEditCloseComents").hide();
        }



        var SensorNo = "";
        $("#btnDualAuthOk").on("click", function () {
            debugger;
            $('#divDualAuthOverlay').hide();
            var cmd = "";
            var sensorNo = $('#hdnSensorNo').val().split('/');
            if ($('#btnBypassName').text().trim() == 'Bypass') {
                cmd = 'byp';
                cmd = cmd + '|' + sensorNo[0];

            }
            else {
                cmd = 'uby';//unbypass                                                             
                cmd = cmd + "|" + sensorNo[0];
            }

            var url = '@Url.Action("DualAuth")';
            jQuery.ajax({
                url: url,
                type: 'POST',
                data: {

                    sLogin: $('#LoginID').val(),
                    sPassword: $('#Password').val(),
                    sComments: $('#Comments').val()

                },
                success: function (data) {
                    if (data.Success) {
                        if (data.ResultRes == "Successs") {
                            jQuery.ajax({
                                url: '@Url.Action("BypassUnBypass")',
                                type: 'POST',
                                data: {
                                    sloginid: $('#LoginID').val(),
                                    sPassword: $('#Password').val(),
                                    sCmd: cmd
                                },
                                success: function (data) { $('#lblPControlErrorBypassRequest').html(data.Text); }
                            });
                        }
                        else {
                            $('#AuthResult').html(data.ResultRes);
                            $('#divDualAuthOverlay').show();
                            $('#divDualAuthPopUp').show();
                        }
                    }
                }
            });
        });
        $("#btnCancel").on("click", function () {
            $('#divDualAuthOverlay').hide();
            $('#divDualAuthPopUp').hide();
        });
        $("#btnBypassSensor").on("click", function () {
            debugger;
            if ($('#btnBypassName').text().trim() == 'Bypass') {
                $('#divDualAuthOverlay').show();
                $('#divDualAuthPopUp').show();//authentication needed
            }
            else {
                var cmd = "";
                var sensorNo = $('#hdnSensorNo').val().split('/');
                cmd = 'uby';//unbypass                                                             
                cmd = cmd + "|" + sensorNo[0];

                jQuery.ajax({
                    url: '@Url.Action("BypassUnBypass")',
                    type: 'POST',
                    data: {

                        sloginid: $('#LoginID').val(),
                        sPassword: $('#Password').val(),
                        sCmd: cmd
                    },
                    success: function (data) { $('#lblPControlErrorBypassRequest').html(data.Text); }
                });
            }
        });



