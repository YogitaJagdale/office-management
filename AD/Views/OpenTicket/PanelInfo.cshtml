            }
        });
    });

    $("#BtnSave").on("click", function () {

        debugger;
        var Notepad = $('#txtNotepad').val();
        $.ajax({
            type: "POST",
            traditional: true,
            url: "SaveNotepad",
            data: { sNotepadData: Notepad },
            success: function (data) {
                if (Notepad != "") {
                    if (data.Success) {
                        $('#TicketHistory').html(data.TicketHistory);
                        $('#PanelHistory').html(data.PanelHistory);
                        $('#SaveStat').html(data.SaveStat);
                        alert('Save text successfully');
                    }
                }
            },
            error: function (reponse) {
                alert("error : " + reponse);
            }
        });
    });

    $("#BtnCamera_Click").on("click", function () {
        //debugger;

        $("#divLoader").show();
        $.ajax({
            type: "POST",
            traditional: true,
            url: '@Url.Action("CameraActiveDeterrence", "ADPartial")',
            success: function (data) {
                //  debugger;
                $("#divLoader").hide();
                $('#partialView').attr("style", "display: block");
                $('#partialView').html(data);
            },
            error: function (reponse) {
                debugger;
                $("#divLoader").hide();
                alert("error : " + reponse);
            }
        });
    });
